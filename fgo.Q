Sub Main()
    Init 
    MainLoop
End Sub

Sub Init()
    ResizeWindow
    
    // 创建工作目录
    Plugin.Bkgnd.SendString 参数1,参数2

    cd = ".\FGO\"
    Plugin.File.CreateFolder cd
    cd = cd & ww & "x" & wh & "\"
    Plugin.File.CreateFolder cd
End Sub

Sub MainLoop()
    Dim rect, arr, x, y, w, h, n
	
    Log "开始运行"
    rundate = Day(Date)
	
    ResetIdle 10
    While True
        rect = Plugin.Window.GetWindowRect(hWnd)
        arr = Split(rect, "|")
        x = Clng(arr(0))
        y = Clng(arr(1))
        w = Clng(arr(2)) - x
        h = Clng(arr(3)) - y
        If x <> wx Or y <> wy Or w <> ww Or h <> wh Then 
            ResizeWindow 
        End If
        Select Case FormMain.Game.ListIndex
        Case 0  // FGO
            Robot
        Case 1  // CQ
            CQRobot 
        End Select
    		
        If FormMain.UnSleep.Value = 1 Then 
            UnSleep
        End If
        RangeDelayEx 1000
    Wend
End Sub

Sub Robot()
    If Day(Date) <> rundate Then 
        MsgBox "日期变更，请重新定位Quest并启动脚本", vbSystemModal, "提示"
        Log "日期变更，运行结束"
        ExitScript 
        Exit Sub
    End If
	
    If CheckQuest(False) Then 
        playedRec = False
        RangeDelayEx 500
        EnterQuest 
    ElseIf CheckApNotEnough(False) Then
        If buyApTimesLeft > 0 Then 
            Log "可购买" & buyApTimesLeft & "次AP"
            BuyAp
            buyApTimesLeft = buyApTimesLeft - 1
            FormMain.buyApTimesLeft.Text = CStr(buyApTimesLeft)
        Else
            Log "AP购买次数不足，休息" & FormMain.rest.Text & "分钟"
            CloseBuyAp 
            HaveARest
        End If
    ElseIf CheckSupport(False) Then
        ChooseSupport FormMain.equips.Text
    ElseIf CheckStartQuest(False) Then
        StartQuest
    ElseIf CheckAttack(False) Then
        If FormMain.PlayRec.Value = 1 And Not playedRec Then 
            playedRec = True
            PlayRec3
            Exit Sub
        End If
        
        For i = 0 To 2
        	ChooseTarget Int(Mid(FormMain.AttackOrder.Text, 3 - i, 1)) - 1
        	RangeDelayEx 200
        Next
        
        If FormMain.CastSpell.Value = 1 Then
            For i = 0 To 29
                While CheckAndCastSkill("Skill" & i)
                    RangeDelayEx 500
                    If CheckChooseSkillTarget(False) Then 
                        ChooseSkillTarget
                    End If
                    RangeDelayEx 2500
                Wend
            Next
        End If
        Attack 
        RangeDelayEx 1000
        ChooseCards 
    ElseIf CheckChooseSkillTarget(False) Then
        ChooseSkillTarget 
    ElseIf CheckCloseAttack(False) Then
        CloseAttack 
    ElseIf CheckCloseServant(False) Then
        CloseServant 
    ElseIf CheckCloseTip(False) Then
        CloseTip
    ElseIf CheckCloseNew(False) Then
        CloseNew 
    ElseIf CheckRetreat(False) Then
        Retreat
    ElseIf CheckFriendship(False) Then
        RangeDelayEx 1000
        TouchScreen
    ElseIf CheckExp(False) Then
        RangeDelayEx 1000
        TouchScreen 
    ElseIf CheckNextStep(False) Then
        NextStep 
    ElseIf CheckNoSupport(False) Then
        NoSupport 
    ElseIf CheckNewFriend(False) Then
        NewFriend 
    ElseIf CheckSkip(False) Then
        RangeDelayEx 1000
        Skip 
    ElseIf CheckNextStep(False) Then
        NextStep 
    Else 
        Idle
    End If
End Sub

Sub Log(str)
    TracePrint str
    If FormMain.Log.ListCount >= 100 Then 
        FormMain.Log.RemoveItem 0
    End If
    FormMain.Log.AddItem str
    FormMain.Log.ListIndex = FormMain.Log.ListCount - 1
End Sub

Function FindWindow()
    Dim rect, arr, winType
	
    winType = Split(FormMain.WinType.List, "|")(FormMain.WinType.ListIndex)
    hWnd = GetScreenWindow(FormMain.winTitle.Text, winType)
    If hWnd = 0 Then 
        FindWindow = False
        MsgBox "找不到渲染窗口", vbSystemModal, "提示"
        Log "找不到渲染窗口，运行结束"
        ExitScript
        Exit Function
    End If
	
    rect = Plugin.Window.GetWindowRect(hWnd)
    arr = Split(rect, "|")
	
    wx = Clng(arr(0))
    wy = Clng(arr(1))
    ww = Clng(arr(2)) - wx
    wh = Clng(arr(3)) - wy
    wix = 0
    wiy = 0
	
    Log "窗口坐标: " & "(" & wx & ", " & wy & ")"
    Log "窗口尺寸: " & ww & " x " & wh
	
    FormMain.CreateTemplatePic.Caption = "创建模板图像(" & ww & "x" & wh & "):"
	
    If mode = 1 Then 
        // 后台模式
		
        hEventWnd = GetEventWindow(FormMain.winTitle.Text, winType)
        If hEventWnd = 0 Then 
            FindWindow = False
            MsgBox "找不到模拟器窗口", vbSystemModal, "提示"
            Log "找不到模拟器窗口，运行结束"
            ExitScript
            Exit Function
        End If

        rect = Plugin.Window.GetWindowRect(hEventWnd)
        arr = Split(rect, "|")
		
        ewx = Clng(arr(0))
        ewy = Clng(arr(1))
        eww = Clng(arr(2)) - ewx
        ewh = Clng(arr(3)) - ewy
        wix = wx - ewx
        wiy = wy - ewy
		
        Log "窗口偏移: " & "(" & wix & ", " & wiy & ")"
    End If
    FindWindow = True
End Function

Sub ResizeWindow()
    Dim w, h, x, y, iw, ih
    w = Int(FormMain.windowWidth.Text)
    h = Int(FormMain.windowHeight.Text)
	
    FindWindow 
    If ww = w And wh = h Then
        Exit Sub
    End If
			
    x = ewx
    y = ewy
    Plugin.Window.Hide hEventWnd
    Plugin.Window.Move hEventWnd, 0, 0
    iw = w - ww
    ih = h - wh
			
    Log "自动校正窗口尺寸"
    Plugin.Bkgnd.MoveTo hEventWnd, eww - 1, ewh / 2
    Plugin.Bkgnd.LeftDown hEventWnd, eww - 1, ewh / 2
    Plugin.Bkgnd.MoveTo hEventWnd, eww - 1 + iw, ewh / 2
    Plugin.Bkgnd.LeftUp hEventWnd, eww - 1 + iw, ewh / 2
    Delay 50
    Plugin.Window.Move hEventWnd, x, y
    Plugin.Window.Restore hEventWnd
    Plugin.Window.Show hEventWnd
    FindWindow 
End Sub

Function GetScreenWindow(title, winType)
    Select Case winType
    Case "句柄"
        GetScreenWindow = Clng(title)
    Case "夜神"
        GetScreenWindow = GetScreenWindowNox(title)
    Case "海马玩"
        GetScreenWindow = GetScreenWindowDroid4X(title)
    Case "逍遥"
        GetScreenWindow = GetScreenWindowMEmu(title)
    Case Else
        GetScreenWindow = 0
    End Select
End Function

