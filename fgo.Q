    Case "2.0主线"
        questPic = "QuestMain2.0"
    Case "幕间"
        questPic = "QuestInterlude"
    Case "强化"
        questPic = "QuestStrengthen"
    Case "自定义"
        questPic = "QuestCustom"
    End Select
    
    Dim res
    If create Then 
        CheckQuest = True
        For i = 0 To 1
