Function dx2wy(dx)
    // 逍遥模拟器窗口设定中，设备中x坐标转换为Windows中y坐标
    // dx:0->dw, wy:wy+wh->wy
    dx2wy = wy + wh - dx * wh / dw
End Function

Function dy2wx(dy)
    // 逍遥模拟器窗口设定中，设备中x坐标转换为Windows中y坐标
    // dy:0->dh, wx:wx->wx+ww
    dy2wx = wx + dy * ww / dh
End Function

Function RangeDelay()
    Dim dur
    
    dur = Rnd() * 100 + 60
    Delay dur
    
    RangeDelay = dur
End Function

Function RangeDelayEx(base)
    Dim dur
	
    dur = Rnd() * 0.5 * base
    If dur > 1000 Then 
        dur = 1000
    End If
    dur = base + dur
    Delay dur
    
    RangeDelayEx = dur
End Function

Sub ResetIdle(max)
    idleCount = 0
    maxIdle = max
End Sub

Sub Idle()
    If idleCount >= maxIdle Then 
    	Skip
        //TouchScreen 
    Else 
        idleCount = idleCount + 1
    End If
End Sub

Sub UnSleep()
    x = dy2wx(Rnd() * (dh - 10) + 10)
    y = dx2wy(Rnd() * (dw - 10) + 10)
    
    MoveTo x, y
End Sub

Sub Touch(x, y)
    idleCount = 0
	
    Select Case mode
    Case 0
        TouchFg x, y
    Case 1
        TouchBg x, y
    Case Else
    End Select
End Sub

Sub TouchFg(x, y)
    MoveTo x, y
    LeftDown 1
    Delay Rnd() * 100 + 100
    LeftUp 1
End Sub

Sub TouchBg(x, y)
    x = x - wx + wix
    y = y - wy + wiy
	
    Plugin.Bkgnd.MoveTo hEventWnd, x, y
    Plugin.Bkgnd.LeftDown hEventWnd, x, y
    Delay Rnd() * 100 + 100
    Plugin.Bkgnd.MoveTo hEventWnd, x, y
    Plugin.Bkgnd.LeftUp hEventWnd, x, y
End Sub

Sub RangeTouch(dx0, dy0, dx1, dy1)
    // 设备坐标系，左下角到右上角
    Dim x, y
    
    x = dy2wx(Rnd() * (dy1 - dy0) + dy0)
    y = dx2wy(Rnd() * (dx1 - dx0) + dx0)
    
    Touch x, y
End Sub


Sub RangeTouchW(x0, y0, x1, y1)
    // Windows坐标系，左上角到右下角
    Dim x, y
    
    x = Rnd() * (x1 - x0) + x0
    y = Rnd() * (y1 - y0) + y0
    
    Touch x, y
End Sub


// ======== 游戏逻辑 ========
Function CheckPicW(x0, y0, x1, y1, name, create)
    // 根据Windows坐标检测图像，左上角到右下角
    Dim findX, findY, fullName
    If create Then 
        If Not Plugin.File.IsFileExist(cd & name) Then
            Plugin.Pic.PrintScreen x0, y0, x1, y1, cd & name
            Log "创建" & name
            CheckPicW = True
        Else 
            Log name & "已存在"
            CheckPicW = False
        End If
    Else
        findX = 0
        findY = 0
        fullName = cd & name
    	
        FindPic x0 - 1, y0 - 1, x1 + 1, y1 + 1, fullName, matchAccuracy * 0.1, findX, findY
        If findX > 0 And findY > 0 Then 
            Log ">>" & name & "匹配"
            CheckPicW = True
        Else 
            CheckPicW = False
        End If
    End If
End Function

Function CheckPic(dx0, dy0, dx1, dy1, name, create)
    // 根据设备坐标检测图像，左下角到右上角
    //Log "检测" & name
    If Len(cd) = 0  Then 
        Init 
    End If
    
    Dim x0, y0, x1, y1
	
    x0 = dy2wx(dy0)
    y0 = dx2wy(dx1)
    x1 = dy2wx(dy1)
    y1 = dx2wy(dx0)
    CheckPic = CheckPicW(x0, y0, x1, y1, name, create)
End Function

