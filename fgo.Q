Function CQCheckBossEnd(create)
    Dim dx0, dy0, dx1, dy1

    dx0 = .421875
    dy0 = .769444444444444
    dx1 = .6890625
    dy1 = .9
    CQCheckBossEnd = CheckPic(dx0, dy0, dx1, dy1, "CQBossEnd.bmp", create)
End Function

Sub CQBossEnd()
    Dim i, dx0, dy0, dx1, dy1
    
    For i = 0 To 5
        dx0 = .909375
        dy0 = .361111111111111
        dx1 = .9734375
        dy1 = .847222222222222
        RangeTouch dx0, dy0, dx1, dy1
        RangeDelayEx 100
    Next
    
    dx0 = .7109375
    dy0 = .791666666666667
    dx1 = .825
    dy1 = .866666666666667
    RangeTouch dx0, dy0, dx1, dy1
End Sub

Event FormMain.CQBossEnd.Click
    CQCheckBossEnd True
End Event

