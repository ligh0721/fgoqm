Function dx2wy(dx)
    // 逍遥模拟器窗口设定中，设备中x坐标转换为Windows中y坐标
    // dx:0->dw, wy:wy+wh->wy
    dx2wy = wy + wh - dx * wh / dw
End Function

Function dy2wx(dy)
    // 逍遥模拟器窗口设定中，设备中x坐标转换为Windows中y坐标
    // dy:0->dh, wx:wx->wx+ww
    dy2wx = wx + dy * ww / dh
End Function

Function RangeDelay()
    Dim dur
    
    dur = Rnd() * 100 + 60
    Delay dur
    
    RangeDelay = dur
End Function

Function RangeDelayEx(base)
    Dim dur
    
    dur = Rnd() * 0.5 * base
    If dur > 1000 Then 
        dur = 1000
    End If
    dur = base + dur
    Delay dur
    
    RangeDelayEx = dur
End Function

Sub ResetIdle(max)
    idleCount = 0
    maxIdle = max
End Sub

Sub Idle()
    If idleCount >= maxIdle Then 
        Skip
        //TouchScreen 
    Else 
        idleCount = idleCount + 1
    End If
End Sub

Sub UnSleep()
    x = dy2wx(Rnd() * (dh - 10) + 10)
    y = dx2wy(Rnd() * (dw - 10) + 10)
    
    MoveTo x, y
End Sub

Sub Touch(x, y)
    idleCount = 0
    
    Select Case mode
    Case 0
        TouchFg x, y
    Case 1
        TouchBg x, y
    Case Else
    End Select
End Sub

Sub TouchFg(x, y)
    MoveTo x, y
    LeftDown 1
    Delay Rnd() * 100 + 100
    LeftUp 1
End Sub

Sub TouchBg(x, y)
    x = x - wx + wix
    y = y - wy + wiy
    
    Plugin.Bkgnd.MoveTo hEventWnd, x, y
    Plugin.Bkgnd.LeftDown hEventWnd, x, y
    Delay Rnd() * 100 + 100
    Plugin.Bkgnd.MoveTo hEventWnd, x, y
    Plugin.Bkgnd.LeftUp hEventWnd, x, y
End Sub

Sub RangeTouch(dx0, dy0, dx1, dy1)
    // 设备坐标系，左下角到右上角
    Dim x, y
    
    x = dy2wx(Rnd() * (dy1 - dy0) + dy0)
    y = dx2wy(Rnd() * (dx1 - dx0) + dx0)
    
    Touch x, y
End Sub


Sub RangeTouchW(x0, y0, x1, y1)
    // Windows坐标系，左上角到右下角
    Dim x, y
    
    x = Rnd() * (x1 - x0) + x0
    y = Rnd() * (y1 - y0) + y0
    
    Touch x, y
End Sub


