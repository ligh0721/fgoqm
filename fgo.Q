Sub ChooseCards()
    Dim dx0, dy0(5), dx1, dy1, dh0, x0, y0, x1, y1, cards(5), choose(3)
    Dim rnum, gnum, bnum, level
    
    dx0 = 144
    
    dy0(0) = 60
    dy0(1) = 318
    dy0(2) = 573
    dy0(3) = 832
    dy0(4) = 1090
	
    dh0 = 140
    dx1 = 244
	
    y0 = dx2wy(dx1)
    y1 = dx2wy(dx0)
    For i = 0 To 4
        dy1 = dy0(i) + dh0
		
        x0 = dy2wx(dy0(i))
        x1 = dy2wx(dy1)
		
        //		rnum = Plugin.Color.CountColor(x0, y0, x1, y1, "2576FF", 0.8)
        //		gnum = Plugin.Color.CountColor(x0, y0, x1, y1, "32C923", 0.8)
        //		bnum = Plugin.Color.CountColor(x0, y0, x1, y1, "FD8724", 0.8)
        rnum = Plugin.Color.CountColor(x0, y0, x1, y1, "0A3BFF", 0.8)
        gnum = Plugin.Color.CountColor(x0, y0, x1, y1, "2ABC24", 0.8)
        bnum = Plugin.Color.CountColor(x0, y0, x1, y1, "FE922F", 0.8)
		
        If rnum > gnum Then
            If rnum > bnum Then 
                level = 2
            Else 
                level = 1
            End If
        Else
            If gnum > bnum Then 
                level = 0
            Else 
                level = 1
            End If
        End If
		
        cards(i) = level * 10 + i
    Next
	
    For i = 0 To 2
        Dim arr(5), num, maxLevel, index
		
        num = 0
        maxLevel = -1
        For j = 0 To 4
            level = cards(j) \ 10
            If level >= maxLevel Then
                If level > maxLevel Then
                    maxLevel = level
                    num = 0
                End If
                arr(num) = cards(j)
                num = num + 1
            End If
        Next
		
        index = arr(Int(Rnd() * num)) Mod 10
        choose(i) = cards(index) mod 10
        cards(index) = -cards(index)
    Next
	
    // 宝具卡
    Dim cdx0, cdy0(3), cdx1, cdy1, cdh0, cx0, cy0, cx1, cy1, minC, cnum
	
    cdx0 = 458
    
    cdy0(0) = 335
    cdy0(1) = 567
    cdy0(2) = 798
	
    cdh0 = 151
    cdx1 = 546
	
    cy0 = dx2wy(cdx1)
    cy1 = dx2wy(cdx0)
	
    minC = 150
    For i = 0 To 2
        cdy1 = cdy0(i) + cdh0
		
        cx0 = dy2wx(cdy0(i))
        cx1 = dy2wx(cdy1)
		
        cnum = Plugin.Color.CountColor(cx0, cy0, cx1, cy1, "BFFFFF", 0.6)
        //Log "cnum: " & cnum & "/" & minC
        If cnum > minC Then 
            ChooseCard 10 + i
            RangeDelay
        End If
    Next
	
    ChooseCard choose(0)
    RangeDelay
    ChooseCard choose(2)
    RangeDelay 
    ChooseCard choose(1)
End Sub

