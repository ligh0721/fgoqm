    If v < FormMain.MatchAccuracyBar.Min Then 
        v = FormMain.MatchAccuracyBar.Min
    ElseIf v > FormMain.MatchAccuracyBar.Max Then
        v = FormMain.MatchAccuracyBar.Max
    End If
    FormMain.matchAccuracy.Text = CStr(v)
    FormMain.MatchAccuracyBar.Value = v
    matchAccuracy = v
End Event

// CQ
Sub CQRobot()
    Dim i, j
    
    If CQCheckExplore(False) Then 
        CQExplore 
    ElseIf CQCheckBoss(False) Then
        CQBoss 
    ElseIf CQCheckEnterBoss(False) Then
        CQEnterBoss 
    ElseIf CQCheckReadyBoss(False) Then
        CQReadyBoss 
    ElseIf CQCheckApNotEnough(False) Then
        If buyApTimesLeft > 0 Then 
            Log "可购买" & buyApTimesLeft & "次体力"
            CQApNotEnough
            buyApTimesLeft = buyApTimesLeft - 1
            FormMain.buyApTimesLeft.Text = CStr(buyApTimesLeft)
        Else
            Log "体力购买次数不足,休息" & FormMain.rest.Text & "分钟"
            HaveARest
        End If
    ElseIf CQCheckBuyOK(False) Then
        CQBuyOK
    ElseIf CQCheckStart(False) Then
        CQStart 
    ElseIf CQCheckFindPartner(False) Then
        CQFindPartner 
    ElseIf CQCheckFoundPartner(False) Then
        CQFoundPartner 
    ElseIf CQCheckBossEnd(False) Then
        CQBossEnd 
    ElseIf CQCheckLord(False) Then
        CQLord 
    ElseIf CQCheckAttack(False) Then
        For i = 1 To 10
            If CQCheckAttack(False) = False Then
                Exit For
            End If
            For j = 1 To 5
                CQAttack 
                RangeDelayEx 10
            Next
            CQGoddess 
            RangeDelayEx 10
            CQLord 
            RangeDelayEx 10
        Next
    Else 
        //Idle
    End If
End Sub

