Sub HaveARest()
    Dim i
	
    i = 0
    While (i < Int(FormMain.rest.Text)) And (buyApTimesLeft <= 0)
        Delay 30000
        i = i + 0.5
        If FormMain.UnSleep.Value = 1 Then 
            KeyPress "Ctrl", 1
        End If
    Wend
End Sub

Function CheckQuest(create)
    Dim dx0(1), dy0, dx1(1), dy1, questType, questPic
    
    dy0 = 778
    dy1 = 986
    
    dx0(0) = 523
    dx1(0) = 561
    dx0(1) = 569
    dx1(1) = 607
    
	
    questType = Split(FormMain.QuestType.List, "|")(FormMain.QuestType.ListIndex)
    Select Case questType
    Case "种火"
        questPic = "QuestFire"
    Case "棋子"
        questPic = "QuestChess"
    Case "QP"
        questPic = "QuestQP"
    Case "1.0自由"
        questPic = "QuestFree1.0"
    Case "1.5自由"
        questPic = "QuestFree1.5"
    Case "2.0自由"
        questPic = "QuestFree2.0"
    Case "1.0主线"
        questPic = "QuestMain1.0"
    Case "1.5主线"
        questPic = "QuestMain1.5"
    Case "2.0主线"
        questPic = "QuestMain2.0"
    Case "幕间"
        questPic = "QuestInterlude"
    Case "强化"
        questPic = "QuestStrengthen"
    Case "自定义"
        questPic = "QuestCustom"
    End Select
    
    Dim res
    If create Then 
        CheckQuest = True
        For i = 0 To 1
            res = CheckPic(dx0(i), dy0, dx1(i), dy1, questPic & i & ".bmp", create)
            If Not res Then 
                CheckQuest = False
            End If
